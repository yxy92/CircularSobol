classdef bbModel
    properties
        ParName, ParValue, OutputName, OutputType, OutputValue 
    end
    
    methods
        
        function obj = bbModel(val)
            if nargin == 1
                obj.ParValue = val;
            end
        end
        
     
        function Output = singleRun(obj,FcnHandle)
            nout = nargout(FcnHandle);
            [Output{1:nout}]
            varargout= FcnHandle(obj.ParValue);
        end    
    end
end